{
  "key": "",
  "manifest_version": 2,
  "name": "browserpass",
  "description": "Browserpass Chrome extension for zx2c4's pass (password manager).",
  "version": "1.0.7",
  "author": "Danny van Kooten",
  "homepage_url": "https://github.com/dannyvankooten/browserpass",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "icon-lock.png",
    "default_popup": "content.html"
  },
  "permissions": [
    "activeTab",
    "nativeMessaging",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+L"
      }
    }
  }
}
