{
  "key": "",
  "manifest_version": 2,
  "name": "browserpass-ce",
  "description": "Chrome extension for zx2c4's pass (password manager) - Community Edition.",
  "version": "2.0.14",
  "author": "Danny van Kooten",
  "homepage_url": "https://github.com/dannyvankooten/browserpass",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "icon-lock.png",
    "default_popup": "content.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "nativeMessaging",
    "notifications",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    }
  },
  "web_accessible_resources": [
    "otp.html"
  ]
}
